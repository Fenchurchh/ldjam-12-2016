{
    "parser": "babel-eslint",
    "globals": {
        "__DEV__": true,
        "angular": true,
        "WHS": true
    },
    "rules": {
        "react/jsx-quotes": 0,
        "jsx-quotes": [
            2,
            "prefer-double"
        ],
        "indent": [
            0,
            "tab"
        ],
        "comma-dangle": [
            0,
            "never"
        ],
        "semi": 0,
        "space-in-parens": 0,
        "object-curly-spacing": 0,
        "no-var": 1,
        "no-unused-var": 1,
        "prefer-const": 1,
        "vars-on-top": 0,
        "object-shorthand": 0,
        "prefer-arrow-callback": 1,
        "no-trailing-spaces": 0,
        "keyword-spacing": 0,
        "new-cap": 0,
        "prefer-template": 1,
        "spaced-comment": 0,
        "max-len": 1,
        "no-param-reassign": 0,
        "padded-blocks": 0,
        "quotes": 0,
        "func-names": 0,
        "space-before-function-paren": [
            0
        ]
    }
}
